{
  "name": "sushichan.live",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "husky install",
    "eslint": "eslint ./src/**/*.{js,ts,jsx,tsx} --ignore-path .gitignore",
    "eslint:fix": "eslint --fix ./src/**/*.{js,ts,jsx,tsx} --ignore-path .gitignore",
    "eslint:precommit": "eslint --max-warnings 0 --ignore-path .gitignore",
    "fmt": "prettier --write --ignore-path .gitignore .",
    "fmt:precommit": "prettier --check --ignore-path .gitignore",
    "stylelint": "stylelint \"./src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint --fix \"./src/**/*.{css,scss}\"",
    "stylelint:precommit": "stylelint --max-warnings 0 --ignore-path .gitignore",
    "markuplint": "markuplint \"./**/*.{jsx,tsx}\"",
    "markuplint:precommit": "markuplint --no-color --problem-only \"./**/*.{jsx,tsx}\"",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "ncu": "ncu -u --target minor",
    "ncu:latest": "ncu --target latest",
    "tsn": "ts-node --compilerOptions='{\"module\":\"commonjs\"}'"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "npm run eslint:precommit",
      "npm run fmt:precommit"
    ],
    "./**/*.{jsx,tsx}": [
      "npm run eslint:precommit",
      "npm run fmt:precommit",
      "npm run markuplint:precommit"
    ],
    "./**/*.{css,scss}": "npm run stylelint:precommit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "budoux": "^0.5.2",
    "bufferutil": "^4.0.7",
    "client-only": "^0.0.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fetch-site-metadata": "^0.1.3",
    "framer-motion": "^10.16.4",
    "glob": "^10.3.4",
    "gray-matter": "^4.0.3",
    "ie-buster": "^2.0.2",
    "next": "13.4.19",
    "next-mdx-remote": "^4.4.1",
    "nextjs-toploader": "^1.4.2",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-player": "^2.13.0",
    "react-spotify-embed": "^2.0.3",
    "react-tweet": "^3.1.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-katex": "^6.0.3",
    "rehype-pretty-code": "^0.10.1",
    "rehype-slug": "^5.1.0",
    "rehype-toc": "^3.0.2",
    "remark-emoji": "^4.0.0",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-unwrap-images": "^3.0.1",
    "ress": "^5.0.2",
    "server-only": "^0.0.1",
    "sharp": "^0.32.5",
    "shiki": "^0.14.4",
    "string-width": "^6.1.0",
    "textlint": "^13.3.3",
    "textlint-rule-preset-japanese": "^8.0.0",
    "unist-util-visit": "^4.1.2",
    "utf-8-validate": "^6.0.3",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.9.0",
    "@markuplint/react-spec": "^3.10.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "8.44.2",
    "@types/node": "20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "markuplint": "^3.12.0",
    "npm-check-updates": "^16.13.2",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-value-no-unknown-custom-properties": "^4.0.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "4.0.0-rc.50"
  }
}
